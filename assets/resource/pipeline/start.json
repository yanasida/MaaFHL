        "threshold": 0.9,
        "roi": [
          62,
          80,
          104,
          475
        ]
      }
    },
    "action": {
      "type": "Click",
      "param": {
        "target": [
          1179,
          65,
          39,
          33
        ]
      }
    },
    "next": [
      "enterGame"
    ]
  },
  "enterGame": {
