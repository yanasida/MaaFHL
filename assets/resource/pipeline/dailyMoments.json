{
  "momentsStart": {
    "post_delay": 3000,
    "recognition": {
      "type": "TemplateMatch",
      "param": {
        "template": [
          "daily/moments/moments_btn1.png",
          "daily/moments/moments_btn2.png"
        ],
        "threshold": 0.7,
